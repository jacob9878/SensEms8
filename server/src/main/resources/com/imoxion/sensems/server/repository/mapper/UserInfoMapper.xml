<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.imoxion.sensems.server.repository.mapper.UserInfoMapper">
    <select id="getUserList" resultType="com.imoxion.sensems.server.domain.ImbUserinfo">
        select * from imb_userinfo
    </select>

    <select id="getUserInfo" resultType="com.imoxion.sensems.server.domain.ImbUserinfo">
        select * from imb_userinfo where userid = #{userid}
    </select>
</mapper>